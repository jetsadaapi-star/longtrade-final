model email_templates {
  id                  BigInt                   @id @default(autoincrement()) @db.UnsignedBigInt
  name                String                   @db.VarChar(255)
  category            email_templates_category
  subject             String?                  @db.VarChar(255)
  html_content        String                   @db.LongText
  plain_text_content  String?                  @db.Text
  available_variables Json?
  thumbnail_url       String?                  @db.VarChar(255)
  is_active           Boolean                  @default(true)
  is_default          Boolean                  @default(false)
  created_at          DateTime?                @db.Timestamp(0)
  updated_at          DateTime?                @db.Timestamp(0)
  deleted_at          DateTime?                @db.Timestamp(0)

  @@index([category], map: "email_templates_category_index")
}

model enrollments {
  id                  BigInt                     @id @default(autoincrement()) @db.UnsignedBigInt
  user_id             BigInt                     @db.UnsignedBigInt
  course_id           BigInt                     @db.UnsignedBigInt
  coupon_id           BigInt?                    @db.UnsignedBigInt
  enrollment_date     DateTime                   @default(now()) @db.Timestamp(0)
  completion_date     DateTime?                  @db.Timestamp(0)
  status              enrollments_status         @default(pending)
  payment_status      enrollments_payment_status @default(pending)
  progress_percentage Decimal                    @default(0.00) @db.Decimal(5, 2)
  original_price      Decimal                    @db.Decimal(10, 2)
  final_price         Decimal                    @db.Decimal(10, 2)
  created_at          DateTime?                  @db.Timestamp(0)
  updated_at          DateTime?                  @db.Timestamp(0)
  certificates        certificates[]
  coupons             coupons?                   @relation(fields: [coupon_id], references: [id], onUpdate: NoAction, map: "enrollments_coupon_id_foreign")
  courses             courses                    @relation(fields: [course_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "enrollments_course_id_foreign")
  users               users                      @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "enrollments_user_id_foreign")

  @@unique([user_id, course_id], map: "enrollments_user_id_course_id_unique")
  @@index([coupon_id], map: "enrollments_coupon_id_foreign")
  @@index([course_id], map: "enrollments_course_id_foreign")
}

