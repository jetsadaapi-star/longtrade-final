  @@index([user_id, status], map: "user_subscriptions_user_id_status_index")
}

model users {
  id                  BigInt                @id @default(autoincrement()) @db.UnsignedBigInt
  name                String                @db.VarChar(255)
  email               String                @unique(map: "users_email_unique") @db.VarChar(255)
  phone               String?               @db.VarChar(20)
  avatar              String?               @db.Text
  role                users_role            @default(student)
  email_verified_at   DateTime?             @db.Timestamp(0)
  phone_verified_at   DateTime?             @db.Timestamp(0)
  password            String                @db.VarChar(255)
  remember_token      String?               @db.VarChar(100)
  created_at          DateTime?             @db.Timestamp(0)
  updated_at          DateTime?             @db.Timestamp(0)
  activity_logs       activity_logs[]
  article_views       article_views[]
  certificates        certificates[]
  enrollments         enrollments[]
  lesson_completions  lesson_completions[]
  marketing_analytics marketing_analytics[]
  promotion_user      promotion_user[]
  quiz_attempts       quiz_attempts[]
  reviews             reviews[]
  social_accounts     social_accounts[]
  two_factor_auth     two_factor_auth?
  user_profiles       user_profiles[]
  user_subscriptions  user_subscriptions[]
  verification_codes  verification_codes[]
}

model verification_codes {
  id          BigInt                  @id @default(autoincrement()) @db.UnsignedBigInt
  user_id     BigInt                  @db.UnsignedBigInt
  type        verification_codes_type
  code        String                  @db.VarChar(6)
  contact     String                  @db.VarChar(255)
  expires_at  DateTime                @db.Timestamp(0)
  verified    Boolean                 @default(false)
  verified_at DateTime?               @db.Timestamp(0)
  created_at  DateTime?               @db.Timestamp(0)
  updated_at  DateTime?               @db.Timestamp(0)
  users       users                   @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "verification_codes_user_id_foreign")

  @@index([code], map: "verification_codes_code_index")
  @@index([expires_at], map: "verification_codes_expires_at_index")
  @@index([user_id, type], map: "verification_codes_user_id_type_index")
}

