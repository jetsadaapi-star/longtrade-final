generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model activity_logs {
  id           BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  user_id      BigInt?   @db.UnsignedBigInt
  log_name     String?   @db.VarChar(255)
  description  String    @db.Text
  subject_type String?   @db.VarChar(255)
  subject_id   BigInt?   @db.UnsignedBigInt
  event        String?   @db.VarChar(255)
  causer_type  String?   @db.VarChar(255)
  causer_id    BigInt?   @db.UnsignedBigInt
  properties   Json?
  batch_uuid   String?   @db.Char(36)
  ip_address   String?   @db.VarChar(45)
  user_agent   String?   @db.Text
  created_at   DateTime? @db.Timestamp(0)
  updated_at   DateTime? @db.Timestamp(0)
  users        users?    @relation(fields: [user_id], references: [id], onUpdate: NoAction, map: "activity_logs_user_id_foreign")

  @@index([causer_type, causer_id], map: "activity_logs_causer_type_causer_id_index")
  @@index([created_at], map: "activity_logs_created_at_index")
  @@index([log_name], map: "activity_logs_log_name_index")
  @@index([subject_type, subject_id], map: "activity_logs_subject_type_subject_id_index")
  @@index([user_id], map: "activity_logs_user_id_foreign")
}

model api_settings {
  id           BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  category     String    @db.VarChar(255)
  key          String    @unique(map: "api_settings_key_unique") @db.VarChar(255)
  name         String    @db.VarChar(255)
  value        String?   @db.Text
  description  String?   @db.Text
  is_encrypted Boolean   @default(false)
  is_active    Boolean   @default(true)
  created_at   DateTime? @db.Timestamp(0)
  updated_at   DateTime? @db.Timestamp(0)

  @@index([category, is_active], map: "api_settings_category_is_active_index")
}

model article_views {
  id         BigInt   @id @default(autoincrement()) @db.UnsignedBigInt
  article_id BigInt   @db.UnsignedBigInt
  user_id    BigInt?  @db.UnsignedBigInt
  ip_address String?  @db.VarChar(45)
  user_agent String?  @db.Text
  viewed_at  DateTime @db.Timestamp(0)
  articles   articles @relation(fields: [article_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "article_views_article_id_foreign")
  users      users?   @relation(fields: [user_id], references: [id], onUpdate: NoAction, map: "article_views_user_id_foreign")

  @@index([article_id], map: "article_views_article_id_index")
  @@index([article_id, viewed_at], map: "article_views_article_id_viewed_at_index")
  @@index([user_id], map: "article_views_user_id_foreign")
  @@index([viewed_at], map: "article_views_viewed_at_index")
}

model articles {
  id                  BigInt                @id @default(autoincrement()) @db.UnsignedBigInt
  slug                String                @unique(map: "articles_slug_unique") @db.VarChar(255)
  title               String                @db.VarChar(255)
  meta_title          String?               @db.VarChar(255)
  meta_description    String?               @db.Text
  meta_keywords       String?               @db.VarChar(255)
  focus_keyword       String?               @db.VarChar(255)
  og_title            String?               @db.VarChar(255)
  og_description      String?               @db.Text
  og_image            String?               @db.VarChar(255)
  twitter_card        articles_twitter_card @default(summary_large_image)
  twitter_title       String?               @db.VarChar(255)
  twitter_description String?               @db.Text
  twitter_image       String?               @db.VarChar(255)
  canonical_url       String?               @db.VarChar(255)
  schema_type         articles_schema_type  @default(Article)
  schema_data         Json?
  seo_score           Int                   @default(0)
  excerpt             String?               @db.Text
  content             String                @db.LongText
  featured_image      String?               @db.VarChar(255)
  author              String?               @db.VarChar(255)
  read_time           Int?
  is_published        Boolean               @default(false)
  is_featured         Boolean               @default(false)
  published_at        DateTime?             @db.Timestamp(0)
  category_id         BigInt?               @db.UnsignedBigInt
  tags                Json?
  views_count         Int                   @default(0)
  created_at          DateTime?             @db.Timestamp(0)
  updated_at          DateTime?             @db.Timestamp(0)
  deleted_at          DateTime?             @db.Timestamp(0)
  article_views       article_views[]
  categories          categories?           @relation(fields: [category_id], references: [id], onUpdate: NoAction, map: "articles_category_id_foreign")

  @@index([category_id], map: "articles_category_id_foreign")
  @@index([focus_keyword], map: "articles_focus_keyword_index")
  @@index([seo_score], map: "articles_seo_score_index")
}

model bookings {
  id         BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)
}

model cache {
  key        String @id @db.VarChar(255)
  value      String @db.MediumText
  expiration Int
}

model cache_locks {
  key        String @id @db.VarChar(255)
  owner      String @db.VarChar(255)
  expiration Int
}

model categories {
  id              BigInt            @id @default(autoincrement()) @db.UnsignedBigInt
  name            String            @db.VarChar(255)
  slug            String            @unique(map: "categories_slug_unique") @db.VarChar(255)
  description     String?           @db.Text
  type            String            @default("general") @db.VarChar(255)
  icon            String?           @db.VarChar(255)
  color           String?           @db.VarChar(255)
  order           Int               @default(0)
  is_active       Boolean           @default(true)
  created_at      DateTime?         @db.Timestamp(0)
  updated_at      DateTime?         @db.Timestamp(0)
  articles        articles[]
  courses         courses[]
  ebooks          ebooks[]
  expert_advisors expert_advisors[]
  indicators      indicators[]
}

model certificates {
  id                 BigInt      @id @default(autoincrement()) @db.UnsignedBigInt
  user_id            BigInt      @db.UnsignedBigInt
  course_id          BigInt      @db.UnsignedBigInt
  enrollment_id      BigInt      @db.UnsignedBigInt
  certificate_number String      @unique(map: "certificates_certificate_number_unique") @db.VarChar(50)
  issued_date        DateTime    @db.Date
  verification_code  String      @unique(map: "certificates_verification_code_unique") @db.VarChar(100)
  pdf_path           String?     @db.Text
  created_at         DateTime?   @db.Timestamp(0)
  updated_at         DateTime?   @db.Timestamp(0)
  courses            courses     @relation(fields: [course_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "certificates_course_id_foreign")
  enrollments        enrollments @relation(fields: [enrollment_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "certificates_enrollment_id_foreign")
  users              users       @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "certificates_user_id_foreign")

  @@index([course_id], map: "certificates_course_id_foreign")
  @@index([enrollment_id], map: "certificates_enrollment_id_foreign")
  @@index([user_id], map: "certificates_user_id_foreign")
}

model coupons {
  id           BigInt        @id @default(autoincrement()) @db.UnsignedBigInt
  code         String        @unique(map: "coupons_code_unique") @db.VarChar(255)
  description  String?       @db.VarChar(255)
  type         coupons_type  @default(percentage)
  value        Decimal       @db.Decimal(10, 2)
  min_purchase Decimal       @default(0.00) @db.Decimal(10, 2)
  max_discount Decimal?      @db.Decimal(10, 2)
  usage_limit  Int?
  used_count   Int           @default(0)
  valid_from   DateTime?     @db.Timestamp(0)
  valid_until  DateTime?     @db.Timestamp(0)
  is_active    Boolean       @default(true)
  created_at   DateTime?     @db.Timestamp(0)
  updated_at   DateTime?     @db.Timestamp(0)
  deleted_at   DateTime?     @db.Timestamp(0)
  enrollments  enrollments[]

  @@index([code], map: "coupons_code_index")
  @@index([is_active], map: "coupons_is_active_index")
  @@index([valid_from, valid_until], map: "coupons_valid_from_valid_until_index")
}

model course_schedules {
  id         BigInt    @id @default(autoincrement()) @db.UnsignedBigInt
  created_at DateTime? @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)
}

model courses {
  id                 BigInt               @id @default(autoincrement()) @db.UnsignedBigInt
  slug               String               @unique(map: "courses_slug_unique") @db.VarChar(255)
  title              String               @db.VarChar(255)
  description        String?              @db.Text
  meta_title         String?              @db.VarChar(255)
  meta_description   String?              @db.Text
  meta_keywords      String?              @db.Text
  og_image           String?              @db.Text
  og_description     String?              @db.Text
  canonical_url      String?              @db.VarChar(255)
  schema_markup      Json?
  cover              String?              @db.Text
  price              Decimal              @default(0.00) @db.Decimal(10, 2)
  original_price     Decimal?             @db.Decimal(10, 2)
  hours              String?              @db.VarChar(255)
  level              String               @default("beginner") @db.VarChar(255)
  lessons_count      Int                  @default(0)
  what_you_learn     String?              @db.Text
  requirements       String?              @db.Text
  is_published       Boolean              @default(false)
  is_featured        Boolean              @default(false)
  average_rating     Decimal              @default(0.00) @db.Decimal(3, 2)
  reviews_count      Int                  @default(0)
  order              Int                  @default(0)
  category_id        BigInt?              @db.UnsignedBigInt
  unlock_code        String?              @db.VarChar(255)
  sections           Json?
  quiz               Json?
  created_at         DateTime?            @db.Timestamp(0)
  updated_at         DateTime?            @db.Timestamp(0)
  deleted_at         DateTime?            @db.Timestamp(0)
  certificates       certificates[]
  categories         categories?          @relation(fields: [category_id], references: [id], onUpdate: NoAction, map: "courses_category_id_foreign")
  enrollments        enrollments[]
  lesson_completions lesson_completions[]
  lesson_videos      lesson_videos[]
  quiz_attempts      quiz_attempts[]

  @@index([category_id], map: "courses_category_id_foreign")
}

model ebooks {
  id                  BigInt              @id @default(autoincrement()) @db.UnsignedBigInt
  slug                String              @unique(map: "ebooks_slug_unique") @db.VarChar(255)
  title               String              @db.VarChar(255)
  author              String?             @db.VarChar(255)
  pages_count         Int?
  description         String?             @db.Text
  meta_title          String?             @db.VarChar(255)
  meta_description    String?             @db.Text
  meta_keywords       String?             @db.VarChar(255)
  focus_keyword       String?             @db.VarChar(255)
  og_title            String?             @db.VarChar(255)
  og_description      String?             @db.Text
  og_image            String?             @db.VarChar(255)
  twitter_card        ebooks_twitter_card @default(summary_large_image)
  twitter_title       String?             @db.VarChar(255)
  twitter_description String?             @db.Text
  twitter_image       String?             @db.VarChar(255)
  canonical_url       String?             @db.VarChar(255)
  schema_type         ebooks_schema_type  @default(Book)
  schema_data         Json?
  seo_score           Int                 @default(0)
  cover_image         String?             @db.VarChar(255)
  price               Decimal             @default(0.00) @db.Decimal(10, 2)
  original_price      Decimal?            @db.Decimal(10, 2)
  pages               Int?
  format              String              @default("pdf") @db.VarChar(255)
  file_path           String?             @db.VarChar(255)
  preview_link        String?             @db.VarChar(255)
  is_published        Boolean             @default(false)
  access_type         ebooks_access_type  @default(free)
  preview_pages       String?             @db.Text
  preview_percentage  Int                 @default(10)
  is_featured         Boolean             @default(false)
  average_rating      Decimal             @default(0.00) @db.Decimal(3, 2)
  reviews_count       Int                 @default(0)
  downloads_count     Int                 @default(0)
  views_count         Int                 @default(0)
  category_id         BigInt?             @db.UnsignedBigInt
  created_at          DateTime?           @db.Timestamp(0)
  updated_at          DateTime?           @db.Timestamp(0)
  deleted_at          DateTime?           @db.Timestamp(0)
  categories          categories?         @relation(fields: [category_id], references: [id], onUpdate: NoAction, map: "ebooks_category_id_foreign")

  @@index([category_id], map: "ebooks_category_id_foreign")
  @@index([focus_keyword], map: "ebooks_focus_keyword_index")
  @@index([seo_score], map: "ebooks_seo_score_index")
}

model email_campaigns {
  id                 BigInt                          @id @default(autoincrement()) @db.UnsignedBigInt
  name               String                          @db.VarChar(255)
  type               email_campaigns_type
  subject            String                          @db.VarChar(255)
  preview_text       String?                         @db.VarChar(150)
